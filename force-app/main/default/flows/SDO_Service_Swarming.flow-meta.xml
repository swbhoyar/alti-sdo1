                </rightValue>
            </conditions>
            <connector>
                <targetReference>Begin_Swarm</targetReference>
            </connector>
            <label>No Swarm Found</label>
        </rules>
        <rules>
            <name>Swarm_Open</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Find_associated_Swarm_s.Status</leftValueReference>
