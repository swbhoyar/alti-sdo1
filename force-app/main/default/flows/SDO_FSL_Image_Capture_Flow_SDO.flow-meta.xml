    <decisions>
        <name>Is_there_a_Case_or_Not</name>
        <label>Is there a Case or Not</label>
        <locationX>270</locationX>
        <locationY>1550</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>VAR_WODetails.CaseId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Look_Up_Case_Details</targetReference>
            </connector>
            <label>Case</label>
        </rules>
        <rules>
            <name>No_Case</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>VAR_WODetails.CaseId</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_WO_as_Complete</targetReference>
            </connector>
            <label>No Case</label>
        </rules>
    </decisions>
    <decisions>
        <name>Is_WOLI_Complete</name>
        <label>Is WOLI Complete</label>
        <locationX>622</locationX>
        <locationY>998</locationY>
        <defaultConnectorLabel>[Default Outcome]</defaultConnectorLabel>
        <rules>
            <name>Complete</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Mark_as_Complete</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_Complete_Status</targetReference>
            </connector>
            <label>Complete</label>
        </rules>
        <rules>
            <name>Not_Complete</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>Mark_as_Complete</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_In_Progress_Status</targetReference>
            </connector>
            <label>Not Complete</label>
        </rules>
    </decisions>
