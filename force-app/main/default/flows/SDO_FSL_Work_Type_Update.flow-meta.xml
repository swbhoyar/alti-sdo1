    <formulas>
        <name>CalcAverageDelta</name>
        <dataType>Number</dataType>
        <expression>if({!NumSAs} == 0,0,

({!TotalActual} - {!TotalEstimated})/{!NumSAs})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalcAvgActualDuration</name>
        <dataType>Number</dataType>
        <expression>if({!NumSAs} == 0,0,

({!TotalActual} /{!NumSAs}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CalcAvgEstDuration</name>
        <dataType>Number</dataType>
        <expression>if({!NumSAs} == 0,0,

({!TotalEstimated})/{!NumSAs})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>RecommendDuration</name>
        <dataType>Number</dataType>
        <expression>max(ceiling(( {!AvgEstimatedDuration} + {!Delta})/{!DurationIncrement})*{!DurationIncrement},0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>updateMaxDelta</name>
        <dataType>Number</dataType>
        <expression>if(isblank({!thisSA.ActualDuration}) || isblank({!thisSA.DurationInMinutes}),{!MaxDelta},

max({!MaxDelta},{!thisSA.ActualDuration} - {!thisSA.DurationInMinutes}))</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>updateMinDelta</name>
        <dataType>Number</dataType>
        <expression>if(isblank({!thisSA.ActualDuration}) || isblank({!thisSA.DurationInMinutes}),{!MinDelta},
min({!MinDelta},{!thisSA.ActualDuration} - {!thisSA.DurationInMinutes}))</expression>
        <scale>2</scale>
    </formulas>
