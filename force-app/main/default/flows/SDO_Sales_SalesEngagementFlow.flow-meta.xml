    </formulas>
    <formulas>
        <description>Formula used to set default name for Opportunity in create screen. This can be modified or overridden</description>
        <name>OpportunityNameFormula</name>
        <dataType>String</dataType>
        <expression>{!Get_Contact_ID.Account.Name}&amp;&quot; - &quot;&amp;TEXT(MONTH({!$Flow.CurrentDate}))&amp;&quot;/&quot;&amp;TEXT(YEAR({!$Flow.CurrentDate}))</expression>
    </formulas>
    <formulas>
        <description>Formula to return today plus two days in order to auto set tasks 2 days out</description>
        <name>todayPlus2</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}+2</expression>
    </formulas>
    <formulas>
        <description>Formula to return today plus two days in order to auto set opportunity close dates 30 days out</description>
        <name>todayplus30</name>
        <dataType>Date</dataType>
        <expression>{!$Flow.CurrentDate}+30</expression>
    </formulas>
