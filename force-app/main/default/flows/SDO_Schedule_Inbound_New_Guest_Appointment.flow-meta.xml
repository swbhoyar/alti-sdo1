            <name>VirtualYes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>ServiceAppointment.AppointmentType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>video</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>VirtualTerritoryId</leftValueReference>
